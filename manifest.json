{
  "version": "2.1",
  "name": "Chrome-Proxy",
  "description": "William Yap Super Jet Proxy for Google Chrome on Desktop",
  "author": "William Yap",
  "permissions": ["webRequest", "webRequestBlocking", "declarativeWebRequest", "proxy", "*://*/*"],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "off.png",
    "default_title": "Chrome-Proxy: Disabled"
  },
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "options_page": "options.html",
  "manifest_version": 2
}
